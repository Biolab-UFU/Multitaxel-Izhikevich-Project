GPIO_Map gpio_map_FA[NUM_TAXELS] = {
    {GPIOC, GPIO_PIN_7}, {GPIOB, GPIO_PIN_5}, {GPIOB, GPIO_PIN_3}, {GPIOA, GPIO_PIN_4},
    {GPIOB, GPIO_PIN_4},  {GPIOC, GPIO_PIN_2},  {GPIOF, GPIO_PIN_4},  {GPIOB, GPIO_PIN_6},
    {GPIOB, GPIO_PIN_2}, {GPIOD, GPIO_PIN_13}, {GPIOD, GPIO_PIN_12}, {GPIOD, GPIO_PIN_11},
    {GPIOE, GPIO_PIN_2},  {GPIOA, GPIO_PIN_0},  {GPIOB, GPIO_PIN_0}, {GPIOE, GPIO_PIN_0}
};

GPIO_Map gpio_map_SA[NUM_TAXELS] = {
    {GPIOA, GPIO_PIN_5}, {GPIOA, GPIO_PIN_6}, {GPIOA, GPIO_PIN_7}, {GPIOD, GPIO_PIN_14},
    {GPIOD, GPIO_PIN_15},  {GPIOF, GPIO_PIN_12},  {GPIOF, GPIO_PIN_13},  {GPIOE, GPIO_PIN_9},
    {GPIOE, GPIO_PIN_11}, {GPIOF, GPIO_PIN_14}, {GPIOE, GPIO_PIN_13}, {GPIOF, GPIO_PIN_15},
